{
  "executionRoleArn": "arn:aws:iam::518013201776:role/ecsTaskExecutionRole",
  "containerDefinitions": [{
    "logConfiguration": {
      "logDriver": "awslogs",
      "secretOptions": null,
      "options": {
        "awslogs-group": "/ecs/pyconkr",
        "awslogs-region": "ap-northeast-2",
        "awslogs-stream-prefix": "ecs"
      }
    },
    "portMappings": [{
      "hostPort": 8000,
      "protocol": "tcp",
      "containerPort": 8000
    }],
    "secrets": [{
        "valueFrom": "PROD_AWS_ACCESS_KEY_ID",
        "name": "AWS_ACCESS_KEY_ID"
      },
      {
        "valueFrom": "PROD_AWS_SECRET_ACCESS_KEY",
        "name": "AWS_SECRET_ACCESS_KEY"
      },
      {
        "valueFrom": "PROD_AWS_STORAGE_BUCKET_NAME",
        "name": "AWS_STORAGE_BUCKET_NAME"
      }
    ],
    "essential": true,
    "memoryReservation": 512,
    "name": "pyconkr"
  }],
  "placementConstraints": [],
  "memory": "2048",
  "taskRoleArn": "arn:aws:iam::518013201776:role/ecsTaskExecutionRole",
  "family": "pyconkr",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "512"
}