{
  "executionRoleArn": "arn:aws:iam::518013201776:role/ecsTaskExecutionRole",
  "containerDefinitions": [{
    "logConfiguration": {
      "logDriver": "awslogs",
      "secretOptions": null,
      "options": {
        "awslogs-group": "/ecs/pyconkr-dev",
        "awslogs-region": "ap-northeast-2",
        "awslogs-stream-prefix": "ecs"
      }
    },
    "portMappings": [{
      "hostPort": 8000,
      "protocol": "tcp",
      "containerPort": 8000
    }],
    "environment": [{
      "name": "AWS_STORAGE_BUCKET_NAME",
      "value": "pyconkr-dev"
    }],
    "secrets": [{
        "valueFrom": "DEV_AWS_ACCESS_KEY_ID",
        "name": "AWS_ACCESS_KEY_ID"
      },
      {
        "valueFrom": "DEV_AWS_SECRET_ACCESS_KEY",
        "name": "AWS_SECRET_ACCESS_KEY"
      }
    ],
    "essential": true,
    "memoryReservation": 512,
    "name": "pyconkr"
  }],
  "placementConstraints": [],
  "taskRoleArn": "arn:aws:iam::518013201776:role/ecsTaskExecutionRole",
  "family": "pyconkr-dev",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512"
}